<!DOCTYPE html>
<html>
<head>
  <title>BloorsonHoldings</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div id='main-container' class='container'>
    <div class='row'>
      <div class='col-md-10'>
        <h2>Bloorson Holdings</h2>
      </div>
      <div class='col-md-2'>
        <%= link_to 'Register', class: 'nav_link' %> // <%= link_to 'Login', class: 'nav_link' %>
      </div>
    </div>
    <div class='row'>
      <div class='col-md-3'>
      </div>
      <div class='col-md-9'>
        <%= yield %>
      </div>
    </div>
  </div>
</body>
</html>
