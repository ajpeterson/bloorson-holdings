<h4 id='welcome'>Welcome, please views some of the current listings.</h4>
<% @listings.each do |listing| %>
  <div class="listing_container">
    <ul>
      <li class="details"><%= link_to listing.address, listing, class: 'nav_link' %></li>
      <li class="details"><%= "#{listing.city}, #{listing.state}, #{listing.zip}" %></li>
    </ul>
  </div>
<% end %>
