<h4 id='welcome'>Welcome, here are some of the current listings.</h4>
<hr>
<% @listings.each do |listing| %>
  <div class="listing_container">
    <ul>
      <li class="details"><%= listing.address %></li>
      <li class="details"><%= "#{listing.city}, #{listing.state}, #{listing.zip}" %></li>
      <li class="details"><%= listing.description %></li>
    </ul>
  </div>
<% end %>
